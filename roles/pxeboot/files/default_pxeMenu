default menu.c32
prompt 0
timeout 300

menu title Labu PXE Boot Menu

label local
  menu label ^0 - Boot from first hard drive
  com32 chain.c32
  append hd0

label 1
  menu label ^1 - Install VMware ESXi 5.5u3
  kernel esxi/mboot.c32
  append -c esxi/boot.cfg ks=http://192.168.6.6/esxi/ks.cfg

label 2
  menu label ^2 - Install Ubuntu 16.04 KVM
  kernel ubuntu-installer/amd64/linux
  append ks=http://192.168.6.6/ubuntu16/kskvm.cfg vga=normal initrd=ubuntu-installer/amd64/initrd.gz lang=us keymap=us live-installer/net-image=http://192.168.6.6/ubuntu16/install/filesystem.squashfs ramdisk_size=16432 root=/dev/rd/0 rw  --

label 3
  menu label ^3 - Install Ubuntu 16.04
  kernel ubuntu-installer/amd64/linux
  append ks=http://192.168.6.6/ubuntu16/ks2018.cfg vga=normal initrd=ubuntu-installer/amd64/initrd.gz lang=us keymap=us live-installer/net-image=http://192.168.6.6/ubuntu16/install/filesystem.squashfs ramdisk_size=16432 root=/dev/rd/0 rw  --

label 4
  menu label ^4 - Install CentOS 7 KVM
  kernel centos/vmlinuz
  append vga=normal initrd=centos/initrd.img method=http://192.168.6.6/centos ks=http://192.168.6.6/centos/kskvm.cfg ramdisk_size=100000

label 5
  menu label ^5 - Install CentOS 7
  kernel centos/vmlinuz
  append vga=normal initrd=centos/initrd.img method=http://192.168.6.6/centos ks=http://192.168.6.6/centos/ks.cfg ramdisk_size=100000

label 6
  menu label ^6 - Clonzilla
  kernel clonzilla/live/vmlinuz
  append initrd=clonzilla/live/initrd.img boot=live union=overlay username=user config components quiet noswap edd=on nomodeset nodmraid inoeject locales= keyboard-layouts=en_US  ocs_live_run="ocs-live-general" ocs_live_extra_param="" ocs_live_batch=no vga=791 ip= net.ifnames=0  nosplash i915.blacklist=yes radeonhd.blacklist=yes nouveau.blacklist=yes vmwgfx.enable_fbdev=1 fetch=http://192.168.6.6/clonzilla/clonzilla/live/filesystem.squashfs

label 7
  menu label ^7 - gparted
  kernel gparted/live/vmlinuz
  append initrd=gparted/live/initrd.img boot=live union=overlay username=user config components quiet noswap  ip= net.ifnames=0  nosplash fetch=http://192.168.6.6/gparted/live/filesystem.squashfs

label 8
  menu label ^8 - Install Linux Mint 18.2 GUI
  kernel mint18-installer/capsper/vmlinuz
  append boot=casper ks=http://192.168.6.6/mint18/ks.cfg vga=normal initrd=mint18-installer/capsper/initrd.lz lang=us keymap=us live-installer/net-image=http://192.168.6.6/mint18/casper/filesystem.squashfs ramdisk_size=16432 root=/dev/rd/0 rw  --

#  kernel ubuntu-installer/amd64/linux
#  append ks=http://192.168.6.6/ubuntu16/ks.cfg vga=normal initrd=ubuntu-installer/amd64/initrd.gz lang=us keymap=us live-installer/net-image=http://192.168.6.6/ubuntu16/install/filesystem.squashfs ramdisk_size=16432 root=/dev/rd/0 rw  --
